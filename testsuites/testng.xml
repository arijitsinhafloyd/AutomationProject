<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">
<listeners>
	<listener class-name="com.selenium.project.arijit.testcomponents.Listeners"/>
</listeners>


  <test thread-count="5" name="Submit Order Test">
    <classes>
      <class name="com.selenium.project.arijit.tests.StandAloneTest"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Error Validation">
    <classes>
  		<class name="com.selenium.project.arijit.tests.ErrorValidation"/>
  	</classes>
  </test>
</suite> <!-- Suite -->
